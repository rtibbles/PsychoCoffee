{
  "author": "Richard Tibbles",
  "name": "psychocoffee",
  "description": "Ember.js driven online psychological experiment platform",
  "version": "1.2.0",
  "homepage": "https://github.com/rtibbles/PsychoCoffee",
  "repository": {
    "type": "git",
    "url": "git://github.com/rtibbles/PsychoCoffee.git"
  },
  "engines": {
    "node": "~0.6.10 || 0.8 || 0.9"
  },
  "scripts": {
    "test": "./node_modules/karma/bin/karma start --single-run --browsers PhantomJS",
