<div class="row">
  <div class="col-md-6">
    <h4 class="text-capitalize">
        {{title}}
    </h4>
    {{#if randomizable}}
    <br>
    <label>
      Randomized: <input type="checkbox" id="experiment_randomized"{{#if model.randomized}}checked{{/if}}>
    </label>
    {{/if}}
  </div>
  <div class="col-md-6">
    {{description}}
  </div>
</div>
<button class="btn btn-success add-variable">Add Variable</button>
{{#if rows}}
<div class="parameterset">
  <table class="table table-bordered parameter-table">
    {{#each rows}}
      {{#if @first}}
      <thead>
      <tr>
        {{#each this}}
          {{#if @first}}<th class="first"{{else}}<td{{/if}}>{{this.name}}</t{{#if @first}}h{{else}}d{{/if}}>
        {{/each}}
      </tr>
      <tr>
        {{#each this}}
          <th>
            {{#if this.randomizable}}<label class="label label-success randomized-label">Randomized <input type="checkbox" class="form-input randomize" dataname="{{this.name}}"{{#if this.randomized}}checked{{/if}}></label>{{/if}}
          </th>
        {{/each}}
      </tr>
      </thead>
      {{else}}
      <tr>
        {{#each this}}
          {{#if @first}}<th class="first"{{else}}<td datatype="{{this.dataType}}" dataindex="{{this.index}}" dataname="{{this.name}}"{{/if}}>{{this.value}}</t{{#if @first}}h{{else}}d{{/if}}>
        {{/each}}
      </tr>
      {{/if}}
      {{#if @first}}
      <tbody>
      {{/if}}
    {{/each}}
    </tbody>
  </table>
</div>
{{/if}}